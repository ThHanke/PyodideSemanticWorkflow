################################################################################
# CATEGORY STYLING
#
# Default styling for workflow categories (can be overridden per template)
################################################################################

spw:MathematicsCategory a spw:WorkflowCategory ;
    rdfs:label "Mathematics" ;
    css:background-color "#e8f5e9" ;
    css:border-color "#4CAF50" ;
    spw:icon "calculate" .

spw:UnitsCategory a spw:WorkflowCategory ;
    rdfs:label "Units" ;
    css:background-color "#fff3e0" ;
    css:border-color "#FF9800" ;
    spw:icon "swap_horiz" .


################################################################################
# End of UI Metadata
################################################################################
